alias gomodlist="find . -type f ! -path '*/pkg/*' -name go.mod -execdir pwd \; -execdir awk 'BEGIN {p=0}; /require \(/ { p = 1 ; next }; /\)/ { p = 0 ; next }; p { print }' go.mod >> /tmp/gomodlist.txt \; ; grep -Ev '^/|indirect|GoDCApp' /tmp/gomodlist.txt | sort | uniq > /tmp/gomodlist_direct.txt ; awk '{print \"    \"\$1\"@\"\$2\" \\\\\"}' /tmp/gomodlist_direct.txt > /tmp/gomodlist_direct_at.txt ; grep 'indirect' /tmp/gomodlist.txt | grep -v 'GoDCApp' | sort | uniq > /tmp/gomodlist_indirect.txt ; awk '{print \"    \"\$1\"@\"\$2\" \\\\\"}' /tmp/gomodlist_indirect.txt > /tmp/gomodlist_indirect_at.txt ; cat /tmp/gomodlist_direct_at.txt /tmp/gomodlist_indirect_at.txt > /tmp/gomodlist_combined.txt ; cat /tmp/gomodlist_combined.txt | sort | uniq ; rm -rf /tmp/gomodlist*.txt"

